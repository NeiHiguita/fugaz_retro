@model fugaz_retro.Models.CostoEnvio

@{
    ViewData["Title"] = "Detalles del Costo de Envío";
}

<h1>Detalles del Precio de Envío</h1>

<div>
    <h4>Costo de Envío</h4>
    <div>
        <dl class="row">
            <dt class="col-sm-2">
                Ciudad
            </dt>
            <dd class="col-sm-10">
                @Model.Ciudad
            </dd>
            <dt class="col-sm-2">
                Costo
            </dt>
            <dd class="col-sm-10">
                @Model.Costo
            </dd>
        </dl>
    </div>

    <p>
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-secondary">Regresar</a>
    </p>
</div>
